<?xml version="1.0"?>
<record-definition prefix="abc" flat="true">

    <namespaces>
        <namespace prefix="dc" uri="http://purl.org/dc/elements/1.1/" schema="http://dublincore.org/schemas/xmls/qdc/dc.xsd"/>
        <namespace prefix="dcterms" uri="http://purl.org/dc/terms/" schema="http://dublincore.org/schemas/xmls/qdc/dcterms.xsd"/>
        <namespace prefix="europeana" uri="http://www.europeana.eu/schemas/ese/" schema="http://www.europeana.eu/schemas/ese/ESE-V3.3.xsd"/>
        <namespace prefix="abc" uri="http://abmu.org/abm" schema="http://abmu.org/abm.xsd"/>
        <namespace prefix="delving" uri="http://schemas.delving.eu/" schema="delving-1.0.xsd"/>
    </namespaces>

    <root tag="record">
        <!-- Category AB-C -->
        <elem tag="abc:titel" summaryField="TITLE"/>
        <elem tag="abc:birthDate"/>
        <elem tag="abc:deathDate"/>
        <elem tag="abc:beginjaar"/>
        <elem tag="abc:eindjaar"/>
        <elem tag="abc:objectId"/>
        <elem tag="abc:dimensionValue"/>
        <elem tag="abc:dimensionUnit"/>
        <elem tag="abc:dimension"/>
        <elem tag="abc:technique"/>
        <elem tag="abc:material"/>
        <elem tag="abc:deepzoomUrl"/>
        <elem tag="abc:smallThumbnail"/>
        <elem tag="abc:largeThumbnail" summaryField="THUMBNAIL"/>
        <elem tag="abc:description" summaryField="DESCRIPTION"/>
        <elem tag="abc:name"/>
        <elem tag="abc:school"/>
        <elem tag="abc:onderwerp"/>
        <elem tag="abc:collection" /> <!--facetName="collectie" facetPrefix="col" sortable="true"-->
        <elem tag="abc:objectName" /> <!--facetName="object" facetPrefix="obj" sortable="true"-->
        <elem tag="abc:kunstenaar" summaryField="CREATOR"/> <!--facetName="kunstenaar" facetPrefix="knst" sortable="true"-->
        <elem tag="abc:production" /> <!--facetName="periode" facetPrefix="per" sortable="true"-->

        <!--  Vervaardiger Velden -->
        <elem tag="abc:vName"/>
        <elem tag="abc:vOccuppation"/>
        <elem tag="abc:vNationality"/>
        <elem tag="abc:vBirthDate"/>
        <elem tag="abc:vBirthPlace"/>
        <elem tag="abc:vDeathDate"/>
        <elem tag="abc:vDeathPlace"/>
        <elem tag="abc:vBiography"/>

        <elem tag="europeana:dataProvider"/>    <!-- ESE 3.4 -->
        <elem tag="europeana:isShownAt"/>       <!-- ESE 3.4 -->
        <elem tag="europeana:isShownBy"/>       <!-- ESE 3.4 -->
        <elem tag="europeana:object"/>          <!-- ESE 3.4 -->
        <elem tag="europeana:provider"/>        <!-- ESE 3.4 -->
        <elem tag="europeana:rights"/>          <!-- ESE 3.4 -->
        <elem tag="europeana:type"/>            <!-- ESE 3.4 -->
        <elem tag="europeana:unstored"/>        <!-- ESE 3.4 -->
        <elem tag="europeana:uri"/>             <!-- ESE 3.4, not accepted by the office and not part of the ESE XSD-->
        <elem tag="europeana:language"/>        <!-- ESE 3.4, not accepted by the office and not part of the ESE XSD -->
        <elem tag="europeana:country"/>         <!-- ESE 3.4,  not accepted by the office and not part of the ESE XSD -->
        <elem tag="europeana:collectionName"/>  <!-- NOT part of ESE 3.4 -->
        <elem tag="europeana:collectionTitle"/> <!-- NOT part of ESE 3.4 -->
        <elem tag="europeana:source"/>          <!-- NOT part of ESE 3.4 -->
        <elem tag="europeana:year"/>
    </root>

    <facts>
        <fact-ref name="spec" path="/record/europeana:collectionName"/>
        <fact-ref name="name" path="/record/europeana:collectionTitle"/>
        <fact-ref name="country" path="/record/europeana:country"/>
        <fact-ref name="dataProvider" path="/record/europeana:dataProvider"/>
        <fact-ref name="language" path="/record/europeana:language"/>
        <fact-ref name="provider" path="/record/europeana:provider"/>
        <fact-ref name="rights" path="/record/europeana:rights"/>
        <fact-ref name="type" path="/record/europeana:type"/>
    </facts>


    <!--<converters>-->
        <!--<converter multipleOutput="true" call=".extractYear()" path="/icn:creatorYearOfBirth"/>-->
        <!--<converter multipleOutput="true" call=".extractYear()" path="/icn:creatorYearOfDeath"/>-->
        <!--<converter call=".toLocalId(spec)" path="/europeana:uri"/>-->
        <!--<converter multipleOutput="true" call=".extractYear()" path="/delving:year"/>-->
    <!--</converters>-->

    <docs>
    </docs>


</record-definition>


